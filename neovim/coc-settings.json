{
  "java.format.enabled": true,
  "java.format.settings.profile": "GoogleStyle",
  "java.format.onType.enabled": false,
  "java.completion.enabled": true,
  "java.completion.maxResults": 20,
  "java.import.gradle.offline.enabled": false,
  "python.linting.enabled": true,
  "python.linting.mypyEnabled": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.width": 30,
  "explorer.trash.command": "trash-put %s",
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "ts",
    "typescript",
    "json",
    "rust",
    "go",
    "python"
  ],
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["cc", "cpp", "c++", "cuda", "objc", "objcpp"],
      "rootPatterns": [".ccls-root", "compile_commands.json", ".ccls"],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls-cache"
        },
        "client": {
          "snipetSupport": true
        },
        "index": {
          "onChange": true
        },
        "highlight": { "lsRanges": true }
      }
    },
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "settings": {},
      "initializationOptions": {}
    },
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        ".stack.yaml",
        ".hie-bios",
        "BUILD.bazel",
        "cabal.config",
        "package.yaml"
      ],
      "filetypes": ["hs", "lhs", "haskell"],
      "initializationOptions": {
        "languageServerHaskell": {
          "hlintOn": true,
          "maxNumberOfProblems": 10,
          "completionSnippetsOn": true
        }
      }
    }
  },
  "clangd.path": "/Users/htamakos/.config/coc/extensions/coc-clangd-data/install/12.0.0/clangd_12.0.0/bin/clangd",
  "yaml.schemas": {
    "kubernetes": "/*.yaml"
  }
}
